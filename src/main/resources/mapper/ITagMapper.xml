<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="iwishing.ccCommunity.community.mapper.ITagMapper">
<!--    通过标签id查询标签类型-->
    <select id="findTagTypeByTagId" resultType="java.lang.String" parameterType="int">
        select tagtype from tag_table where id=#{tagId};
    </select>
</mapper>
